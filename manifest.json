{
  "manifest_version": 3,
  "name": "Unlock",
  "version": "1.97",
  "description": "Unlock the web",
  "action": {
    "default_icon": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  },
  "permissions": [
    "storage",
    "tabs",
    "tabGroups",
    "sidePanel",
    "webNavigation",
    "declarativeNetRequest",
    "alarms",
    "offscreen",
    "activeTab",
    "scripting"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "externally_connectable": {
    "matches": [
      "https://*.amazonaws.com/*",
      "https://*.digitaloceanspaces.com/*",
      "https://storage.googleapis.com/*"
    ]
  },
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "web_accessible_resources": [
    {
      "resources": [
        "overlay.js",
        "overlay.css",
        "preview.html",
        "page_interceptor.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "side_panel": {
    "default_path": "sidebar.html"
  }
}